{
  "name": "photo-backend",
  "version": "0.1.0",
  "description": "Production-ready, modular backend for a photography platform (NestJS).",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/data-source.ts",
    "seed:admin": "ts-node -r tsconfig-paths/register src/seeds/seed-admin.ts",
    "fix:roles": "ts-node -r tsconfig-paths/register src/scripts/fix-roles-null.ts",
    "create:test-service": "ts-node -r tsconfig-paths/register src/scripts/create-test-service.ts",
    "create:test-service:docker": "docker-compose exec api npm run create:test-service",
    "check:services": "ts-node -r tsconfig-paths/register src/scripts/check-services.ts",
    "check:services:docker": "docker-compose exec api npm run check:services",
    "verify:db": "ts-node -r tsconfig-paths/register src/scripts/verify-db-connection.ts",
    "compare:db": "ts-node -r tsconfig-paths/register src/scripts/compare-db-connections.ts",
    "verify:commit": "ts-node -r tsconfig-paths/register src/scripts/verify-immediate-commit.ts",
    "list:admins": "ts-node -r tsconfig-paths/register src/scripts/list-all-admins.ts",
    "delete:admins": "ts-node -r tsconfig-paths/register src/scripts/delete-all-admins.ts",
    "test:admin": "ts-node -r tsconfig-paths/register src/scripts/test-admin-login.ts",
    "check:admin": "ts-node -r tsconfig-paths/register src/scripts/check-admin-direct.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.676.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.2.0",
    "@nestjs/cache-manager": "^2.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/typeorm": "^10.0.0",
    "argon2": "^0.41.1",
    "axios": "^1.7.0",
    "bull": "^4.12.2",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "joi": "^17.12.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nestjs-pino": "^4.0.0",
    "oci-common": "^2.122.2",
    "oci-objectstorage": "^2.122.2",
    "oci-sdk": "^2.122.2",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "pino": "^8.19.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^11.0.0",
    "razorpay": "^2.9.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "stripe": "^16.10.0",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.24",
    "@types/passport": "^1.0.17",
    "@types/passport-facebook": "^3.0.4",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/passport-oauth2": "^1.8.0",
    "@types/pdfkit": "^0.17.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=18"
  }
}
